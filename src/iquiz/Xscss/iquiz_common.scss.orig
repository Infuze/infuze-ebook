/*QUIZ INTRO SCREEN*/

.iquiz-lesson-intro-container {
    display: flex;
    flex-direction: row;
    flex-wrap: nowrap;
    justify-content: flex-start;
    align-items: flex-start;
    align-content: flex-start;
    height: 80px;
    margin: 40px auto;
    /*max-width: 700px;*/
}
.iquiz-lesson-intro-logo {
    flex: 1;
    display: flex;
    justify-content: flex-end;
    padding: 0 20px 0 0;
    height: inherit;
}
.iquiz-lesson-intro-logo img {
    height: 100%;
    width: auto;
}
.iquiz-lesson-intro-heading {
    flex: 2;
    padding: 0 0 0 20px;
    border-left: solid 2px red;
    height: inherit;
    display: flex;
    flex-direction: column;
    justify-content: center;
}
.iquiz-lesson-intro-heading h1, .iquiz-lesson-intro-heading h2 {
    padding: 4px 0;
    margin: 0;
    font-weight: 600;
}

.iquiz-intro-info-panel {
    margin: 0 auto;
    text-align: center;
}

.iquiz-intro-info-panel p {
    font-size: 18px;
    line-height: 30px;
    padding: 8px 0;
    margin-bottom: 60px;
}

@media only screen and (min-device-width: 375px) and (max-device-width: 667px) and (-webkit-min-device-pixel-ratio: 2) {
    .iquiz-lesson-intro-container {
        display: block;
        height: auto;
        margin: 40px 0;
    }
    .iquiz-lesson-intro-logo {
        display: block;
        height: 90px;
        text-align: center;
    }
    .iquiz-lesson-intro-heading {
        display: block;
        padding: 10px 0;
        border-left: none;
        border-bottom: red 2px solid;
        text-align: center;

    }

}

